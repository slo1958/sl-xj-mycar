#tag WindowCode
	#tag Method, Flags = &h0
		Function CalculateAmountFullCosts() As double
		  
		  var d as Double = tempConvertedAmount
		  
		  if rg_PercentFullCosts.SelectedIndex = 0 then
		    return 0
		    
		  elseif rg_PercentFullCosts.SelectedIndex = 1 then
		    return d
		    
		    
		  else
		    var p as Double = txf_PercentFullCosts.text.trim.ToDouble / 100
		    
		    return d * p
		    
		  end if
		  
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Function CalculateAmountTrip() As double
		  
		  var d as Double = tempConvertedAmount
		  
		  if rg_PercentTripExpenses.SelectedIndex = 0 then
		    return 0
		    
		  elseif rg_PercentTripExpenses.SelectedIndex = 1 then
		    return d
		    
		    
		  else
		    var p as Double = txf_PercentTripExpenses.text.trim.ToDouble / 100
		    
		    return d * p
		    
		  end if
		  
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Function CalculateConvertedAmount() As double
		  
		  var d as Double = txf_OriginalAmount.Text.Trim.ToDouble
		  
		  if cmb_OriginalAmountCurrency.text = "" then 
		    Return d
		    
		  elseif cmb_OriginalAmountCurrency.text = currentCommonData.CommonCurrency then
		    return d
		    
		  else
		    var c as Double = txf_ExchangeRate.text.ToDouble
		    
		    if c = 0 then 
		      return d
		      
		    else
		      return c * d
		      
		    end if
		    
		  end if
		  
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub LoadFromEvent(srcGeneral as clMyCarData, srcVehicle as clVehicle, srcEvent as clEvent)
		  
		  Self.currentEvent = srcEvent
		  
		  Self.LoadUI(srcGeneral, srcVehicle, srcEvent, CarEventTypes.DummyEntry)
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub LoadFromEventType(srcGeneral as clMyCarData, srcVehicle as clVehicle, srcType as CarEventTypes)
		  
		  self.currentEvent = new clEvent(-1)
		  
		  self.LoadUI(srcGeneral, srcVehicle, nil, CarEventTypes.DummyEntry)
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h1
		Protected Sub LoadUI(srcGeneral as clMyCarData, srcVehicle as clVehicle, srcEvent as clEvent, srcType as CarEventTypes)
		  
		  self.currentCommonData = srcGeneral
		  self.currentVehicle = srcVehicle
		  self.currentEvent = srcEvent
		  
		  lbl_MileageUnit.Text = srcGeneral.DistanceUnit
		  lbl_VolumeUnit.Text = srcGeneral.FuelVolumeUnit
		  
		  
		  var tmpCurrentEventType as CarEventTypes 
		  
		  if srcEvent = nil then
		    tmpCurrentEventType = srcType
		    
		    UpdateEventTypePopup(nil)
		    
		    lbl_InternalID.Text = "-"
		    
		  else
		    tmpCurrentEventType = srcEvent.eventType
		    
		    UpdateEventTypePopup(srcEvent)
		    
		    lbl_InternalID.Text = srcEvent.ID.ToString
		    
		  end if
		  
		  UpdateThirdPartyPopup(srcGeneral, srcVehicle, tmpCurrentEventType)
		  
		  
		  
		   
		  //pm_OriginalAmountCurrency
		   
		  
		  // if srcEvent
		  // rg_PercentFu.text = srcEvent.DescriptionllCosts
		  txa_description.text = srcEvent.Description
		  txa_ThirdPartyComments.text = srcEvent.ThirdPartyInfo1
		  
		  // Amount and currency 
		  txf_OriginalAmount.text = srcEvent.AmountInOriginalCurrency.ToString
		  lbl_ConvertedAmountValue.Text = srcEvent.Amount.ToString
		  tempConvertedAmount = srcEvent.Amount
		  
		  if srcEvent.OriginalCurrencyCode = "" then
		    cmb_OriginalAmountCurrency.Text = srcGeneral.CommonCurrency
		    txf_ExchangeRate.text = "1"
		    
		  else
		    cmb_OriginalAmountCurrency.text = srcEvent.OriginalCurrencyCode
		    txf_ExchangeRate.text = srcEvent.CurrencyExchangeRate.ToString
		    
		  end if
		  
		  
		  
		  txf_EventDate.text = srcEvent.DateEventCreated
		  txf_EventInfo.text = srcEvent.EventInfo
		  txf_mileage.text = srcEvent.currentMileage.ToString
		  
		  txf_PercentFullCosts.text = srcEvent.PercentFullCosts.ToString
		  
		  if srcEvent.PercentFullCosts = 0 then
		    rg_PercentFullCosts.SelectedIndex = 0
		    txf_PercentFullCosts.Visible = false
		    
		  elseif srcEvent.PercentFullCosts = 100 then
		    rg_PercentFullCosts.SelectedIndex = 1
		    txf_PercentFullCosts.Visible = false
		    
		  else
		    rg_PercentFullCosts.SelectedIndex = 2
		    txf_PercentFullCosts.Visible = True
		    
		  end if
		  
		  txf_PercentTripExpenses.text = srcEvent.PercentTripCosts.ToString
		  
		  if srcEvent.PercentTripCosts = 0 then
		    rg_PercentTripExpenses.SelectedIndex = 0
		    
		    
		  elseif srcEvent.PercentTripCosts = 100 then
		    rg_PercentTripExpenses.SelectedIndex = 1
		    
		  else
		    rg_PercentTripExpenses.SelectedIndex = 2
		    
		  end if
		  
		  
		  txf_ReminderDate.text = srcEvent.reminderDate
		  txf_ThirdParty.Text = srcEvent.ThirdPartyInfo0
		  cmb_TripName.Text = srcEvent.TripLabel
		  txf_volume.Text = srcEvent.Volume.ToString
		  
		  
		  txf_EventDate.text = DateTime.Now.SQLDate
		  
		  Title = "Event for " +  srcVehicle.CommonDescription
		  
		  
		  UpdateTripPopup(srcGeneral, srcVehicle)
		  
		  UpdateCurrencyPopup(srcGeneral, srcVehicle, srcType)
		  
		  Return
		  
		End Sub
	#tag EndMethod

