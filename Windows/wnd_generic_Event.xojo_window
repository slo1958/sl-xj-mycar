	#tag Method, Flags = &h0
		Sub UpdateCurrencyPopup(srcGeneral as clMyCarData, srcVehicle as clVehicle, srcType as CarEventTypes)
		  
		  var tmp() as string = srcGeneral.GetCurrencies(srcVehicle, srcType)
		  
		  cmb_OriginalAmountCurrency.RemoveAllRows
		  
		  for each s as string in tmp
		    cmb_OriginalAmountCurrency.AddRow(s)
		    
		  next
		  
		  Return
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub UpdateCurrencyVisibility()
		  
		  var needConversion as Boolean = ( cmb_OriginalAmountCurrency.text <> "" ) and ( cmb_OriginalAmountCurrency.text <> currentCommonData.CommonCurrency)
		  
		  lbl_ExchangeRateMultiply.Visible = needConversion
		  txf_ExchangeRate.Visible = needConversion
		   
		  lbl_ConvertedAmountValue.Visible = needConversion
		   
		  lbl_amountConversion.text  =  currentCommonData.CommonCurrency + " per " +  cmb_OriginalAmountCurrency.text
		  lbl_amountConversion.Visible = needConversion
		  
		  Return
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub UpdateEventTypePopup(srcEvent as clEvent)
		  
		  pm_EventType.RemoveAllRows
		  
		  var currentValueIndex as Integer = -1
		  
		  var events() as pair = AllEvents
		  
		  for each p as pair in events
		    pm_EventType.AddRow(p.Right.StringValue)
		    pm_EventType.RowTagAt(pm_EventType.LastAddedRowIndex) = p.left
		    
		    if srcEvent <> nil and integer(srcEvent.eventType) = p.left.IntegerValue then currentValueIndex = pm_EventType.LastAddedRowIndex
		    
		  next
		  
		  if currentValueIndex < 0 then 
		    pm_EventType.SelectedRowIndex = 0
		    
		  else
		    pm_EventType.SelectedRowIndex = currentValueIndex
		    
		  end if
		  
		  Return
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub UpdateFullCostsVisibility()
		  
		  var detailsVisible as Boolean = rg_PercentFullCosts.SelectedIndex = 2
		  
		  lbl_FullCostsValue.Visible = detailsVisible
		  
		  txf_PercentFullCosts.Visible = detailsVisible
		  
		  lbl_percentSignFullCosts.Visible = detailsVisible
		  
		  Return
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub UpdateThirdPartyPopup(srcGeneral as clMyCarData, srcVehicle as clVehicle, srcType as CarEventTypes)
		  
		  pm_ThirdParty.RemoveAllRows
		  
		  var arr() as string = srcGeneral.GetThirdParties(srcVehicle, srcType)
		  
		  if arr.Count = 0 then
		    pm_ThirdParty.Visible = false
		    return 
		    
		  end if
		  
		  for each tmp as string in arr
		    pm_ThirdParty.AddRow(tmp)
		    
		  next
		  
		  pm_ThirdParty.Visible = True
		  
		  return 
		  
		End Sub
	#tag EndMethod

