	#tag Method, Flags = &h0
		Sub UpdateUIBasedOnEventType(evt as CarEventTypes)
		  
		  
		  // Fuel volume visible
		  var fuelVolumeVisible as Boolean = ShouldDisplayVolumeFor(evt)
		  
		  lbl_Volume.Visible = fuelVolumeVisible
		  lbl_VolumeUnit.Visible = fuelVolumeVisible
		  txf_volume.Visible = fuelVolumeVisible 
		  
		  // Reminder date visible
		  var reminderDateVisible as Boolean = ShouldDisplayReminderDateFor(evt)
		  
		  lbl_ReminderDate.Visible = reminderDateVisible
		  txf_ReminderDate.Visible = reminderDateVisible
		  
		  
		  
		  txf_EventInfo.Visible = (evt  = CarEventTypes.Other)
		  
		  return
		End Sub
	#tag EndMethod


