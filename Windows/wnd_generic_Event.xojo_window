	#tag Method, Flags = &h0
		Sub UpdateConvertedAmount()
		  
		  self.tempConvertedAmount = CalculateConvertedAmount()
		  
		  lbl_ConvertedAmountValue.text = "= " + tempConvertedAmount.ToString+ " " + currentCommonData.CommonCurrency
		  
		  UpdateAmountFullCosts
		  
		  UpdateAmountTrip
		  
		  Return
